    <template id="assets_editor" inherit_id="website.assets_editor" name="Editor assets">
        <xpath expr="//link[last()]" position="after">
            <link href="/theme_clean/static/src/less/variables.less" rel="stylesheet" type="text/less"/>
            <link href="/theme_clean/static/src/less/customize_modal.less" rel="stylesheet" type="text/less"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_common/static/src/js/s_progress_bar.js"></script>
        </xpath>
    </template>
</odoo>
